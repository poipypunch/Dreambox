@model IEnumerable<MVCDreambox.Models.Package>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PackageDesc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PackageStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdateDate)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PackageDesc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PackageStatus)
            </td>
            @*<td>
                    @Html.DisplayFor(modelItem => item.CreateBy)
                </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            @*<td>
                    @Html.DisplayFor(modelItem => item.UpdateBy)
                </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.UpdateDate)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.PackageID }) |
                @*@Html.ActionLink("Details", "Details", new { id=item.PackageID }) |*@
                @Html.ActionLink("Delete", "Delete", new { id = item.PackageID })
            </td>
        </tr>
    }

</table>
