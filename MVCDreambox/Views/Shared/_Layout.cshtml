</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Dreambox", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    @*@if (Session["UserID"].ToString() =="Admin")
                        {*@
                    <li>@Html.ActionLink("User", "Index", "tbUser")</li>
                    <li>@Html.ActionLink("Payment", "Index", "Payment")</li>
                    <li>@Html.ActionLink("Package", "Index", "Package")</li>                    
                    @*}*@
                    <li>@Html.ActionLink("Channel", "Index", "Channel")</li>
                    <li>@Html.ActionLink("Member Type", "Index", "MemberType")</li>
                    <li>@Html.ActionLink("Member", "Index", "Member")</li>
                    <li>@Html.ActionLink("Category", "Index", "Category")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderSection("featured", required: false)
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Dreambox Solution</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
